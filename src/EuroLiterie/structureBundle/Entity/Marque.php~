<?php
namespace EuroLiterie\structureBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 *@ORM\Entity(repositoryClass="MarqueRepo")
 *@ORM\Table(name="marques")
 */
class Marque
{
    
    /**
     *@ORM\Id
     *@ORM\Column(type="integer")
     */
    protected $idMarque;

    /**
     *@ORM\Column(type="string")
     */
    protected $nomMarque;

    /**
     *@ORM\Column(type="string")
     */
    protected $pngUrl;

    /**
     *@ORM\Column(type="string")
     */
    protected $content;

    /**
     *@ORM\Column(type="string")
     */
    protected $marqueLien;
}
